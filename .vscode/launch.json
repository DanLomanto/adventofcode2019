{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Debug Test - Chrome Locally",
      "program": "${workspaceRoot}/node_modules/testcafe/bin/testcafe.js",
      "args": [
        "chrome",
        "${relativeFile}",
        "-e"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceRoot}/node_modules/**/*.js",
      ],
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Debug Test - Mobile Locally",
      "program": "${workspaceRoot}/node_modules/testcafe/bin/testcafe.js",
      "args": [
        "chrome:emulation:device=iphone X;touch=true",
        "${relativeFile}",
        "-e"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceRoot}/node_modules/**/*.js",
      ],
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Debug Test - Chrome Browserstack",
      "program": "${workspaceRoot}/node_modules/testcafe/bin/testcafe.js",
      "args": [
        "browserstack:Chrome:Windows 10",
        "${relativeFile}",
        "-e"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceRoot}/node_modules/**/*.js",
      ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "name": "Debug Test - iPhone Browserstack",
      "program": "${workspaceRoot}/node_modules/testcafe/bin/testcafe.js",
      "args": [
        "browserstack:iPhone XS",
        "${relativeFile}",
        "-e"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceRoot}/node_modules/**/*.js",
      ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch test",
      "runtimeVersion": "12.5.0",
      "program": "${workspaceFolder}/yea.js"
    }
  ]
}
